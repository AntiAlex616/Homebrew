<elements>
	<info>
		<name>Languages.xml</name>
		<description>Languages for Homebrew content</description>
		<author>AntiAlex</author>
		<update version='1.0'>
			<file name="KH.xml" url="https://raw.githubusercontent.com/AntiAlex616/Homebrew/master/languages/Languages.xml" />
		</update>
	</info>
	
	<element name="Heartless" type="Language" source="Homebrew" id="ID_KH_LANGUAGE_HEARTLESS">
		<description>
			<p>Typically spoken by the various forms of Heartless.</p>
		</description>
		<setters>
			<set name="exotic">true</set>
			<set name="speakers">Heartless</set>
			<set name="script"></set>
		</setters>
	</element>
	
	<element name="Nobody" type="Language" source="Homebrew" id="ID_KH_LANGUAGE_NOBODY">
		<description>
			<p>Typically spoken by the various forms of Nobody.</p>
		</description>
		<setters>
			<set name="exotic">true</set>
			<set name="speakers">Nobody</set>
			<set name="script"></set>
		</setters>
	</element>

</elements>
